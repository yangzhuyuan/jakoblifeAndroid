{
    // launch.json 配置了启动调试时相关设置，configurations下节点名称可为 app-plus/h5/mp-weixin/mp-baidu/mp-alipay/mp-qq/mp-toutiao/mp-360/
    // launchtype项可配置值为local或remote, local代表前端连本地云函数，remote代表前端连云端云函数
    "version" : "0.0",
    "configurations" : [
        {
            "app" : {
                "launchtype" : "remote"
            },
            "app-plus" : {
                "launchtype" : "remote"
            },
            "default" : {
                "launchtype" : "local"
            },
            "h5" : {
                "launchtype" : "remote"
            },
            "mp-weixin" : {
                "launchtype" : "local"
            },
            "provider" : "aliyun",
            "type" : "uniCloud"
        },
        {
            "customPlaygroundType" : "local",
            "localRepoPath" : "D:/workdemo/jianbo",
            "packageName" : "com.work.jakob",
            "playground" : "custom",
            "type" : "uni-app:app-android"
        },
        {
            "playground" : "custom",
            "type" : "uni-app:app-ios"
        }
    ]
}
